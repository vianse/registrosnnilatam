wb = xlsx_package.workbook

wb.add_worksheet(name: "Usuarios_Registrados") do |sheet|
  # this is the head row of your spreadsheet
  sheet.add_row %w(id Nombre Apellido NombreCompleto Email Evento_ID)
  
  # each user is a row on your spreadsheet
  @registers_events.each do |user|
    sheet.add_row [user.id, user.firtsName, user.lastName, user.nickName, user.email, user.eventId]
  end
end