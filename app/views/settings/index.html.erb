<p id="notice"><%= notice %></p>

<h1>Settings</h1>

<table>
  <thead>
    <tr>
      <th>Token</th>
      <th>Urlendpoint</th>
      <th>Apikey</th>
      <th>Urlreditectiontokenempty</th>
      <th>Urlredirectiontokenerror</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.token %></td>
        <td><%= setting.urlEndPoint %></td>
        <td><%= setting.apiKey %></td>
        <td><%= setting.urlReditectionTokenEmpty %></td>
        <td><%= setting.urlRedirectionTokenError %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
